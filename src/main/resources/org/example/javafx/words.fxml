    <AnchorPane prefHeight="700.0" prefWidth="214.0">
        <TextField fx:id="textField1" layoutY="50" prefHeight="100" prefWidth="150" />
        <TextField fx:id="textField2" layoutX="200" layoutY="50" prefHeight="100" prefWidth="150" />
        <Button fx:id="button" layoutX="150" layoutY="150.0" mnemonicParsing="false"  onMouseClicked="#reverse" text="->" />
    </AnchorPane>
</VBox>
